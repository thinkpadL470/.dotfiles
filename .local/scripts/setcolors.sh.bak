#!/bin/dash
footcremaining='\nselection-foreground=\nselection-background=\nsearch-box-no-match=\nsearch-box-match=\njump-labels=\nurls=\n'
footcgen=$(printf "[colors]\n$(cat ~/.cache/wal/colors-wal-st.h | \
  sed 's/\("\|,\|#\|\ \ \)//g ; s/\ *=\ /=/g ; s/\/\*.*\*\///g ; /\(^$\|unsigned\|}\|colorname\|back\|\[258\]\)/d
    s/\[257\]=/foreground=/g
    s/\[256\]=/background=/g
    s/\[0\]=/regular0=/g
    s/\[1\]=/regular1=/g
    s/\[2\]=/regular2=/g
    s/\[3\]=/regular3=/g
    s/\[4\]=/regular4=/g
    s/\[5\]=/regular5=/g
    s/\[6\]=/regular6=/g
    s/\[7\]=/regular7=/g
    s/\[8\]=/bright0=/g
    s/\[9\]=/bright1=/g
    s/\[10\]=/bright2=/g
    s/\[11\]=/bright3=/g
    s/\[12\]=/bright4=/g
    s/\[13\]=/bright5=/g
    s/\[14\]=/bright6=/g
    s/\[15\]=/bright7=/g')${footcremaining}"
footcgen | tr "\n" " "
