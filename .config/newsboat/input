# [NEWSBOAT_KEYBIND_CONFIG]
  # [KEY_FUNCTIONS]
    unbind-key	-a
    bind-key	  r	mark-feed-read
    bind-key	  m	macro-prefix
    bind-key	  s	sort
    bind-key	  Q	hard-quit
    bind-key	  U	reload-all
    bind-key	  S	rev-sort
    bind-key	  O	open-in-browser
    bind-key	  L	redraw
    bind-key    F open-search
  # [NAVIGATION]
    bind-key   ^U halfpageup
    bind-key   ^D halfpagedown
    bind-key    g home
    bind-key    G end
    bind-key    n	next-unread
    bind-key    p	prev-unread
    bind-key    x	close-dialog
    bind-key    h	quit
    unbind-key	h	feedlist
    bind-key    j	down
    bind-key	  k	up
    bind-key	  l	open
  # [MACROS]
    macro v set browser "setsid -f mpv --terminal=no --mute=yes" ; open-in-browser ; set browser librewolf
    macro c set browser "wl-copy" ; open-in-browser
